set hlsearch
set ignorecase

noremap j 5j
noremap k 5k

set hintchars=abcdefghijklmnopqrstuvwxyz
highlight Hint font-size:15px;background-color: #333333;color: #ffffff; font-family: "Monaco", "Century Gothic"; font-weight: bold;


