[user]
	name = k_somemo
	email = kntsmy@hotmail.co.jp
[merge]
	ff = false

[alias]
	ignore = !"f() { local s=$1; shift; \
		while [ $# -gt 0 ]; do s="$s,$1"; shift; done;\
			curl "http://gitignore.io/api/$s"; }; f"

	st = status
	co = checkout
	cm = commit
	br = branch
	res = reset
	l = log
	di = diff
	logp = log -p
	ad = add
	ps = push
	mg = merge
	lograph = log --graph --pretty=medium-reverse
	stash-unstage = git stash save â€“-keep-index
[color]
	ui = true
[core]
	editor = vim
[pretty]
	medium-reverse = format:'%C(red reverse)%d%Creset%C(white reverse) %h% Creset %C(green reverse) %an %Creset %C(cyan)%ar%Creset%n%C(white bold)%w(80)%s%Creset%n%n%w(80,2,2)%b'
